Config { font = "-misc-fixed-*-*-*-*-10-*-*-*-*-*-*-*"
	, bgColor = "black"
	, fgColor = "grey"
	, position = TopSize L 95 16
	, lowerOnStart = True
	, commands = [ Run Weather "EGPF" ["-t","<station>: <tempC>C","-L","18","-H","25","--normal","green","--high","red","--low","lightblue"] 36000
		, Run Network "eth0" ["-t","Net: <rx>, <tx>","-H","200","-L","10","-h","#FF8980","-l","#B1FF7A","-n","#FFFFCC"] 10
		, Run MultiCpu ["-t","CPU: <total0> <total1> <total2> <total3>","-L","30","-H","60","-h","#FF8980","-l","#B1FF7A","-n","#FFFFCC","-w","3"] 10
		, Run Memory ["-t","Mem: <usedratio>%","-H","8192","-L","4096","-h","#FF8980","-l","#B1FF7A","-n","#FFFFCC"] 10
		, Run Date "%a %b %_d %Y %H:%M:%S" "date" 10
    , Run Com "/home/max/.bin/wireless.sh" [] "wifi" 30
		, Run Com "/home/max/.bin/audio.sh" [] "volume" 1
    , Run Com "/home/max/.bin/battery.sh" [] "battery" 30
	]
	, sepChar = "%"
	, alignSep = "}{"
	, template = "%multicpu% | %memory% | %wifi% }{ %volume% | %battery% | %eth0% | <fc=#EDB64E>%date%</fc>"
}
